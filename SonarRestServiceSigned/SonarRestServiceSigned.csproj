<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>8.0.0-alpha007</Version>
    <TargetFrameworks>netstandard2.0;net452</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>8.0.0.0</AssemblyVersion>
    <FileVersion>8.0.0.0</FileVersion>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\AssemblyKey.snk</AssemblyOriginatorKeyFile>
    <OutputPath>..\BuildDrop\Signed</OutputPath>
    <AssemblyName>SonarRestService</AssemblyName>
    <RootNamespace>SonarRestService</RootNamespace>
    <PackageId>SonarRestServiceSigned</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RestSharp" Version="106.5.4" />
  </ItemGroup>

  <Import Project="..\SonarRestServiceShared\SonarRestServiceShared.projitems" Label="Shared" />

</Project>
